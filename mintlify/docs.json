{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Bytebase Docs",
  "colors": {
    "primary": "#4F46E5",
    "light": "#6366F1",
    "dark": "#4F46E5"
  },
  "favicon": "/favicon.svg",
  "icons": {
    "library": "lucide"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Home",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "introduction/what-is-bytebase",
              "introduction/supported-databases",
              "introduction/use-cases",
              "poc",
              "onboarding",
              "concepts/data-model",
              "faq"
            ]
          },
          {
            "group": "Get Started",
            "pages": [
              {
                "group": "Install",
                "pages": [
                  "get-started/self-host",
                  "get-started/upgrade",
                  "get-started/install/external-postgres",
                  "get-started/install/external-url",
                  "get-started/cloud"
                ]
              },
              {
                "group": "Quickstart",
                "pages": [
                  "get-started/step-by-step/deploy-with-docker",
                  "get-started/step-by-step/register-accounts",
                  "get-started/step-by-step/set-up-environments",
                  "get-started/step-by-step/add-an-instance",
                  "get-started/step-by-step/create-a-project",
                  "get-started/step-by-step/change-schema",
                  "get-started/step-by-step/query-data"
                ]
              }
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "https://raw.githubusercontent.com/bytebase/bytebase/refs/heads/main/proto/gen/grpc-doc/openapi.yaml"
      }
    ],
    "global": {
      "anchors": []
    }
  },
  "logo": {
    "light": "/logo/logo-full.png",
    "dark": "/logo/logo-full-white.png"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "bytebase.com",
      "href": "https://www.bytebase.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/bytebase",
      "github": "https://github.com/bytebase/bytebase",
      "linkedin": "https://linkedin.com/company/bytebase"
    }
  },
  "errors": {
    "404": {
      "redirect": false
    }
  },
  "seo": {
    "metatags": {
      "robots": "noindex"
    }
  }
}
